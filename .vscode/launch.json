  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/fint.dll",
      "args": ["test/hello/hello.dll"],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole"
    },
    {
      "name": "Mono Launch (console)",
      "type": "mono",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/fint.dll",
      "args": ["test/hello/hello.dll"],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole"
    }
  ]
}
